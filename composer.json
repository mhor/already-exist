{
    "name": "mhor/already-extract",
    "type": "project",
    "description": "Little tool to check if archives are already extracted in a folder",
    "license": "MIT",
    "authors": [
        {
            "name": "mhor",
            "email": "maxime.horcholle@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.6",
        "symfony/finder": "~2.3",
        "symfony/filesystem": "~2.3",
        "symfony/console": "~2.3",
        "alchemy/zippy": "0.2.*@dev"

    },
    "suggest": {
        "ext-zip": "Allows to uncompress zip files using ZipArchive",
        "ext-rar": "Allows to uncompress rar files using RarArchive"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "symfony/process": "~2.3"
    },
    "autoload": {
        "psr-0": {
            "": "src/"
        }
